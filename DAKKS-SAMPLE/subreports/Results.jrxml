      )
      ? ( "± " + $F{tol_pos}.trim().replaceFirst("^[+-]","")
          + ($F{tol_pos_p}==null || $F{tol_pos_p}.trim().isEmpty() ? "" : " " + $F{tol_pos_p}.trim())
