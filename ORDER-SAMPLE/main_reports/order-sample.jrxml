CONCAT_WS(" ", IF(c.K4606 IS NULL OR TRIM(c.K4606) = "", NULL, c.K4606), IF(c.K4604 IS NULL OR TRIM(c.K4604) = "", NULL, c.K4604)) AS order_address, CONCAT_WS(" ", bc.K4606, bc.K4604) AS billing_company, CONCAT_WS(" ", dc.K4606, dc.K4604) AS delivery_company, b.customer_contact_id, tc.type AS contact_type,
CONCAT_WS("\n", IF(tc.label IS NULL OR TRIM(tc.label) = "", NULL, tc.label), tc.name, IF(tc.contact IS NULL OR TRIM(tc.contact) = "", NULL, tc.contact), tc.street, CONCAT_WS(" ", tc.zip, tc.location)) AS customer_contact, COALESCE(tc.street, "") AS customer_street, CONCAT_WS(" ", COALESCE(tc.zip, ""), COALESCE(tc.location, "")) AS customer_address,
CONCAT_WS(", ", IF(tc.label IS NULL OR TRIM(tc.label) = "", NULL, tc.label), tc.name, IF(tc.contact IS NULL OR TRIM(tc.contact) = "", NULL, tc.contact), tc.street, CONCAT_WS(" ", tc.zip, tc.location)) AS customer_contact_for_billing,
b.invoice_customer_KTAG, b.delivery_customer_KTAG, b.invoice_contact_id, b.delivery_contact_id,
CONCAT_WS("\n", IF(ti.label IS NULL OR TRIM(ti.label) = "", NULL, ti.label), ti.name, IF(ti.contact IS NULL OR TRIM(ti.contact) = "", NULL, ti.contact), ti.street, CONCAT_WS(" ", ti.zip, ti.location)) AS billing_contact, COALESCE(ti.street, "") AS billing_street, CONCAT_WS(" ", COALESCE(ti.zip, ""), COALESCE(ti.location, "")) AS billing_address,
CONCAT_WS("\n", IF(td.label IS NULL OR TRIM(td.label) = "", NULL, td.label), td.name, IF(td.contact IS NULL OR TRIM(td.contact) = "", NULL, td.contact), td.street, CONCAT_WS(" ", td.zip, td.location)) AS delivery_contact, COALESCE(td.street, "") AS delivery_street, CONCAT_WS(" ", COALESCE(td.zip, ""), COALESCE(td.location, "")) AS delivery_address,
CONCAT_WS(", ", IF(td.label IS NULL OR TRIM(td.label) = "", NULL, td.label), td.name, IF(td.contact IS NULL OR TRIM(td.contact) = "", NULL, td.contact), td.street, CONCAT_WS(" ", td.zip, td.location)) AS delivery_contact_for_billing,
IF(a.pos_number IS NULL, "", COALESCE(a.pos_number, "1.00")) AS pos_number, a.tax, IF($P{Dokumenttyp} IN ('Billing','Rechnung') AND a.article_type = 'type', 0, ROUND(COALESCE(a.price, 0), 2)) AS price, CONCAT_WS(" | ", IF(a.complexity IS NULL OR TRIM(a.complexity) = "", NULL, a.complexity), sa.article_name) AS article_name, IF(a.complexity IS NULL OR TRIM(a.complexity) = "", "", a.complexity) AS complexity, COALESCE(a.quantity, "1.00") AS quantity, COALESCE(a.unit, "1.00") AS unit, a.article_type, IF(a.discount IS NULL OR TRIM(a.discount) = "", 0, TRIM(ROUND(a.discount, 2))+0) AS discount, CONCAT_WS(" | ", IF(a.complexity IS NULL OR TRIM(a.complexity) = "", NULL, a.complexity), COALESCE(t.type_name,"")) AS type_name,
CONCAT_WS("\n", CONCAT_WS(" | ", IF(a.complexity IS NULL OR TRIM(a.complexity) = "", NULL, a.complexity), i.I4202, i.I4203), CONCAT_WS(" | ", i.I4201, IF(i.I4244 IS NULL, NULL, i.I4244))) AS inventory_name, COALESCE( i.I4204,"") AS I4204, COALESCE(i.I4202,"") AS I4202, COALESCE( i.I4206,"") AS I4206, COALESCE(i.I4203,"") AS I4203, DATE_FORMAT(DATE_ADD(CURDATE(), INTERVAL COALESCE(b.W4103, 0) DAY), "%d.%m.%Y") AS Added_Date
FROM $P!{PrefixTable}booking b
LEFT JOIN $P!{PrefixTable}collective_invoices ci ON ci.invoice_uID=b.uID
LEFT JOIN $P!{PrefixTable}customers c on b.customer_KTAG=c.KTAG
LEFT JOIN $P!{PrefixTable}customers bc on b.invoice_customer_KTAG=bc.KTAG
LEFT JOIN $P!{PrefixTable}customers dc on b.delivery_customer_KTAG=dc.KTAG
LEFT JOIN $P!{PrefixTable}contact tc on tc.uid=b.customer_contact_id
LEFT JOIN $P!{PrefixTable}contact ti on ti.uid=b.invoice_contact_id
LEFT JOIN $P!{PrefixTable}contact td on td.uid=b.delivery_contact_id
LEFT JOIN $P!{PrefixTable}article a
	ON (b.uid=a.booking_uid OR ci.other_booking_uID=a.booking_uid)
	AND a.article_type IN ('$P!{Articletype1}','$P!{Articletype2}','$P!{Articletype3}')
LEFT JOIN $P!{PrefixTable}standard_article sa on a.article_ID=sa.uID
LEFT JOIN $P!{PrefixTable}prices p on a.article_ID=p.pID
LEFT JOIN $P!{PrefixTable}inventory i on a.article_ID=i.MTAG
LEFT JOIN $P!{PrefixTable}types t on p.type_tID=t.tID
WHERE b.uID=$P{Auftragsid}
ORDER BY a.pos_number ASC,
		CASE WHEN a.article_type = 'inventory' THEN 0
     		 WHEN a.article_type = 'article' THEN 1
     	ELSE 2 END]]>
