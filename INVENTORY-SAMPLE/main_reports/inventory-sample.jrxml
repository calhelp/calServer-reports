	<queryString>
		<![CDATA[SELECT i.*, c.*, b.*, l.*, cal.*
FROM $P!{PrefixTable}inventory i
LEFT JOIN $P!{PrefixTable}customers c
  ON i.ktag = c.KTAG
LEFT JOIN $P!{PrefixTable}barcode b
  ON i.MTAG = b.barcode_key
LEFT JOIN (
  SELECT * FROM $P!{PrefixTable}location
  WHERE L2815 = 1
) l
  ON l.MTAG = i.MTAG
LEFT JOIN (
  SELECT * FROM $P!{PrefixTable}calibration
  WHERE C2339 = 1
) cal
  ON cal.MTAG = i.MTAG
WHERE i.MTAG = $P{P_MTAG}]]>
	</queryString>
