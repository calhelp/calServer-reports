	        <queryString language="SQL">
                <![CDATA[SELECT
  i.`I4201` AS inv_I4201,
  COALESCE(TRIM(c.`K4601`), '-') AS cust_K4601,
  COALESCE(TRIM(c.`K4602`), '-') AS cust_K4602,
  COALESCE(TRIM(l.`L2802`), '') AS loc_L2802,
  COALESCE(TRIM(l.`L2803`), '') AS loc_L2803,
  COALESCE(TRIM(l.`L2831`), '') AS loc_L2831,
  COALESCE(TRIM(l.`L2830`), '') AS loc_L2830,
  CASE
    WHEN cal.`C2301` LIKE '%/%' THEN STR_TO_DATE(cal.`C2301`, '%m/%d/%Y')
    ELSE cal.`C2301`
  END AS cal_C2301,
  CASE
    WHEN cal.`C2303` LIKE '%/%' THEN STR_TO_DATE(cal.`C2303`, '%m/%d/%Y')
    ELSE cal.`C2303`
  END AS cal_C2303
FROM $P!{PrefixTable}inventory i
LEFT JOIN $P!{PrefixTable}customers c ON i.KTAG = c.KTAG
LEFT JOIN $P!{PrefixTable}location l ON l.MTAG = i.MTAG AND l.L2815 = 1
LEFT JOIN $P!{PrefixTable}calibration cal ON cal.MTAG = i.MTAG
  AND cal.C2339 = 1
WHERE i.MTAG = $P{P_MTAG}]]>
